{

   name=     h2o
   
   output_style_options= {
      real_precision= 5
   }
   
   basis_directory= ./basis_sets
   basis_name= cc-pVDZ
   
   charge=   0
   multiplicity= 1

   E_field= 0.000000 0.000000  0.050000
   
   atoms= {

      keys= { label= { units= angstrom } pos= }

      data= {
         O  0.00000 0.000000  1.20300
         C  0.00000 0.000000  0.00000
         H  0.93454 0.000000 -0.57831
         H -0.93454 0.000000 -0.57831
      }

      do { use_interpolators= NO }

   }

   put


   scfdata= {

      initial_density= promolecule

      kind=            rhf

      direct=          on

      convergence= 0.000010

      diis= { convergence_tolerance= 0.000010 }

   }
   
   scf

   becke_grid= {
     accuracy= high
   }

   put_1e_properties

   delete_scf_archives

}

