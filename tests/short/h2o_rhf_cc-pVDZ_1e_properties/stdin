{

   name=     h2o
   
   output_style_options= {
      real_precision= 4
   }
   
   basis_directory= ./basis_sets
   basis_name= cc-pVDZ
   
   charge=   0
   multiplicity= 1
   
   atoms= {

      keys= { label= { units= angstrom } pos= }

      data= {
         O     -0.028260    0.000029    0.000000
         H      0.599917    0.767685    0.000000
         H      0.599941   -0.767609    0.000000
      }

      do { use_interpolators= NO }

   }

   put


   scfdata= {

      initial_density= promolecule

      kind=            rhf

      direct=          on

      convergence= 0.00001

      diis= { convergence_tolerance= 0.00001 }

   }
   
   scf

   becke_grid= {
     accuracy= high
   }

   put_1e_properties

   delete_scf_archives

}

