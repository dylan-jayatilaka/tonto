{                                 
   name= IAM  

   ! Process the CIF
    CIF= {
       file_name= alanin.cif
    }
   process_CIF

   name= IAM  

   crystal= {
 
     xray_data= {

         optimise_extinction= NO
         correct_dispersion=  false
         optimize_scale_factor= true
         wavelength= 1.54178 Angstrom
         REDIRECT alanin_tonto.hkl

         f_sigma_cutoff= 4.0

         refine_H_U_iso= yes

         tol_for_shift_on_esd= 0.0001

         show_fit_output= TRUE
         show_fit_results= TRUE
         

      }

   }

   set_isotropic_h_adps 
   ! Geometry    
   put

   IAM_refinement

}
