{ 

   keyword_echo_on

   name= my_job

   ! Process the CIF
   CIF= {
       file_name= AS_Lcys_har_0m.cif
    }

   process_CIF

   name= my_job

   crystal= {    
      xray_data= {   
         optimise_extinction= false
         optimise_scale_factor= yes
         correct_dispersion= yes
         wavelength= 0.71073 Angstrom
         REDIRECT tonto.data
         f_sigma_cutoff= 0.0001
         tol_for_shift_on_esd= 0.010000
         refine_H_U_iso= yes

         show_fit_output= true
         show_fit_results= true

      }  
   }  

   ! Geometry    
   put

   IAM_refinement

}
