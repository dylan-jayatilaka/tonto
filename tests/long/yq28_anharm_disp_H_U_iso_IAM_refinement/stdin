{

   name= yq28

   output_style_options= { real_precision= 4 }

   CIF= { file_name= ./YQ28.cif }
   process_CIF

   name= yq28

   dispersion_coefficients= {
      C    0.00000    0.00000
      Cl   0.00000    0.00000
      H    0.00000    0.00000
      N    0.00000    0.00000
      O    0.00000    0.00000
      S    2.00000    1.00000 
   }

   crystal= {

      xray_data= {

         wavelength= 0.41030 Angstrom

         optimise_extinction= NO
         correct_dispersion=  TRUE

         refine_4th_order_for_atoms= { S Cl1 Cl2 } ! <---
       
 	 REDIRECT xd.hkl

         f_sigma_cutoff= 4.0

         refine_H_U_iso= YES

         tol_for_shift_on_esd= 0.000001

         do_residual_cube= YES

         show_fit_output= FALSE
         show_fit_results= TRUE
         

      }

   }
   
   set_isotropic_h_adps

   put

   ! Refine atoms with invarioms
   IAM_refinement

}
