         k1 = k_pts(uf:ul,1)
         k2 = k_pts(uf:ul,2)
         k3 = k_pts(uf:ul,3)
         T  =    tf(uf:ul)

         ! Make symmetry phases
         .make_phases_for_symop(u,phase,.translated_symop,refl)

         ! Loop over the points
         do k = 1,n_refl

            k1k = k1(k)
            k2k = k2(k)
            k3k = k3(k)
            ph = phase(k) * T(k)
            facr = Fr(k)
            facc = Fc(k)

            do p = 1,n_p

               kr = k1k*r1(p)+k2k*r2(p)+k3k*r3(p)
               val  = exp(I*kr)*ph
               valr =  real(val)
               valc = aimag(val)

               X(p) = X(p) + valr*facr + valc*facc

            end

         end

         ! Add up inverted symop contribution, if any
         if ( all(.inverted_symop/=.unique_SF_symop(u)) ) cycle

         ! Loop over the points
         do k = 1,n_refl

            k1k = k1(k)
            k2k = k2(k)
            k3k = k3(k)
            ph = phase(k) * T(k)
            facr = Fr(k)
            facc = Fc(k)

            do p = 1,n_p

               kr = k1k*r1(p)+k2k*r2(p)+k3k*r3(p)
               val  = exp(-I*kr)*ph
               valr =  real(val)
               valc = aimag(val)

               X(p) = X(p) + valr*facr + valc*facc

            end

         end

      end

      PARALLEL_SUM(X)

