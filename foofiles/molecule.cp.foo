      do u = 1,3
      do v = 1,3
         alpha(u,v) = mu2(u,v) - fnm1*mu1(u)*mu1(v)
      end
      end
      alpha = fac1*alpha

      do u = 1,3
      do v = 1,3
      do w = 1,3
         val1 = mu3(u,v,w)
         val2 = mu1(u)*mu2(v,w) + mu1(v)*mu2(w,u) + mu1(w)*mu2(u,v)
         val3 = mu1(u)*mu1(v)*mu1(w)
         beta(u,v,w) =  val1 - fnm1*val2 + TWO*fnm2*val3
      end
      end
      end
      beta = fac2*beta

      stdout.flush
      stdout.text("==========================================")
      stdout.text("Jayatilaka-Jha-Munshi (JJM) polarisability")
      stdout.text("==========================================")

