      b10vec( 2) = beta(1,1,2)
      b10vec( 3) = beta(1,2,2)
      b10vec( 4) = beta(2,2,2)
      b10vec( 5) = beta(1,1,3)
      b10vec( 6) = beta(1,2,3)
      b10vec( 7) = beta(2,2,3)
      b10vec( 8) = beta(1,3,3)
      b10vec( 9) = beta(2,3,3)
      b10vec(10) = beta(3,3,3)

      stdout.put(b10vec)

      stdout.flush
      stdout.text("JJM Vector dipole hyperpolarisability:")
      stdout.flush
      beta.set_vector_hyperpolarisability(bvec)

      stdout.put(bvec)
      stdout.flush
      stdout.show("JJM |beta| =",bvec.norm)


      P.destroy
      Oxyz.destroy
      Ozzy.destroy; Ozzx.destroy
      Oyyz.destroy; Oyyx.destroy
      Oxxz.destroy; Oxxy.destroy
