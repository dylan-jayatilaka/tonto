   new_r_Becke88_x_energy_density(E,N0,Nx,Ny,Nz) ::: private
   ! The restricted Becke 88 exchange energy density.
      self :: IN
      E  :: VEC{REAL}, INOUT
      N0,Nx,Ny,Nz :: VEC{REAL}, IN

      beta,beta6,const,two_m43,two_m13,rho,rho_13,rho_43,gx,gy,gz,gg,x,x2 :: REAL
      i :: INT

      beta = 0.0042d0  ! beta parameter
      beta6 = SIX*beta
      const = THREE/TWO * (THREE/(FOUR*PI))**THIRD
      two_m13 = TWO**(-THIRD)
      two_m43 = HALF*two_m13

      do i = 1,N0.dim
         rho = N0(i)
         if (rho<.rho_cutoff) cycle
         rho_13 = rho**(THIRD)
         rho_43 = two_m43 * rho * rho_13
         gx = Nx(i)
         gy = Ny(i)
         gz = Nz(i)
         gg = HALF*sqrt(gx*gx+gy*gy+gz*gz)
       ! x  = min(gg/rho_43,.rho_cutoff)
         x  = gg/rho_43
         x2 = x*x
         E(i) = E(i) - two_m13*rho_13*(const+beta*x2/(ONE+beta6*x*log(x+sqrt(ONE+x2))))
      end

   end

   new_r_Becke88_x_energy_density_parallel(E,N0,Nx,Ny,Nz) ::: private
   ! The restricted Becke 88 exchange energy density.
      self :: IN
      E  :: VEC{REAL}, INOUT
      N0,Nx,Ny,Nz :: VEC{REAL}, IN

      beta,beta6,const,two_m43,two_m13,rho,rho_13,rho_43,gx,gy,gz,gg,x,x2 :: REAL
      i :: INT
      parallel_E :: VEC{REAL}(E.dim)

      beta = 0.0042d0  ! beta parameter
      beta6 = SIX*beta
      const = THREE/TWO * (THREE/(FOUR*PI))**THIRD
      two_m13 = TWO**(-THIRD)
      two_m43 = HALF*two_m13

      parallel_E = ZERO

      parallel do i = 1,N0.dim
         rho = N0(i)
         if (rho<.rho_cutoff) cycle
         rho_13 = rho**(THIRD)
         rho_43 = two_m43 * rho * rho_13
         gx = Nx(i)
         gy = Ny(i)
         gz = Nz(i)
         gg = HALF*sqrt(gx*gx+gy*gy+gz*gz)
       ! x  = min(gg/rho_43,.rho_cutoff)
         x  = gg/rho_43
         x2 = x*x
         parallel_E(i) = two_m13*rho_13*(const+beta*x2/(ONE+beta6*x*log(x+sqrt(ONE+x2))))
      end

      if(DO_IN_PARALLEL) then
