   put_theta_info
   ! Output Theta-space information
      self :: IN

   ENSURE(.atom_a.associated, "No Roby atom A data")
   ENSURE(.atom_b.associated, "No Roby atom B data")
   ENSURE(.atom_ab.associated,"No Roby atom AB data")

      n,i,j :: INT
