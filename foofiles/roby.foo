!      rsa, rsb :: REAL
!
!   ENSURE(.theta_C.associated,"no theta_C covalent orbitals")
!   ENSURE(.eigen_C.associated,"no eval_C covalent eigenvalues")
!   ENSURE(.theta_C.dim1==.n_bf_ab,"wrong shape, theta_C")
