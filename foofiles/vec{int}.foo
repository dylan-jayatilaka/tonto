   to_sequence(first,last,step) ::: leaky, pure
   ! Set "self" to a seqience from "first" up to "last" with "step"
   ! spacing if present, or spacing 1 if not.
      self :: PTR
      first :: INT, IN
      last :: INT, IN
      step :: INT, optional, IN

      i,n,s,val :: INT

      ! Step
      s = 1
      if (present(step)) s = step

      ! Sequence length
      n = (last - first)/s
      if (n<0) then; n = 0
      else;          n = n + 1
      end

      ! The sequence
      allocate(self(n))

      i   = 0
      val = first

      ! Make it
      do 

         if (val>last) exit

         i = i + 1

         self(i) = val

         val = val + s

      end

   end

