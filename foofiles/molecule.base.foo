   serialize(filename)
   ! Write out data from sbf file
      filename :: STR
      datafile :: DATAFILE
      labels :: STR(len=1), dimension(:,:), allocatable
      i, label_length = 1 :: INT

    if (tonto.is_master_processor) then

      ! how long is the longest label
      do i = 1, .atom.size
         if (len(trim(.atom(i).label)) > label_length) label_length = len(trim(.atom(i).label))
      end do

      ! need to convert the labels to an MxN character array
      allocate(labels(.atom.size, label_length))
      do i = 1, .atom.size
         labels(i, :) = .atom(i).label.character_array(label_length)
      end do

      datafile.open(filename)
      call datafile%sbf%add_dataset(sbf_Dataset("name", trim(.name)))
      call datafile%sbf%add_dataset(sbf_Dataset("basis_name", trim(.basis_name)))
      call datafile%sbf%add_dataset(sbf_Dataset("atomic_labels", labels))
      call datafile%sbf%add_dataset(sbf_Dataset("atomic_numbers", .atom(:).atomic_number))
      call datafile%sbf%add_dataset(sbf_Dataset("atomic_coordinates", .atom.coordinate_vector))
      call datafile%sbf%add_dataset(sbf_Dataset("charge_multiplicity", [.charge, .spin_multiplicity]))


      ! SCF data
      ! TODO should really check if it's created and DONE not just if it's unrestricted
      if (.scfdata.associated) then

         select case (.scfdata.spinorbital_kind)

         case ("restricted")
            call datafile%sbf%add_dataset(sbf_Dataset("scf_kind", trim("restricted")))
            call datafile%sbf%add_dataset(sbf_Dataset("density_matrix", .density_matrix.restricted))
            call datafile%sbf%add_dataset(sbf_Dataset("molecular_orbitals", .molecular_orbitals.restricted))

         case ("unrestricted")
            call datafile%sbf%add_dataset(sbf_Dataset("scf_kind", trim("unrestricted")))
            call datafile%sbf%add_dataset(sbf_Dataset("density_matrix.alpha", .density_matrix.alpha))
            call datafile%sbf%add_dataset(sbf_Dataset("density_matrix.beta", .density_matrix.beta))
            call datafile%sbf%add_dataset(sbf_Dataset("molecular_orbitals.alpha", .molecular_orbitals.alpha))
            call datafile%sbf%add_dataset(sbf_Dataset("molecular_orbitals.beta", .molecular_orbitals.alpha))

         case ("general")
            call datafile%sbf%add_dataset(sbf_Dataset("scf_kind", trim("general")))
            call datafile%sbf%add_dataset(sbf_Dataset("density_matrix.general", .density_matrix.general))
            call datafile%sbf%add_dataset(sbf_Dataset("molecular_orbitals.general", .molecular_orbitals.general))

         case default
            call datafile%sbf%add_dataset(sbf_Dataset("scf_kind", trim("restricted")))
            call datafile%sbf%add_dataset(sbf_Dataset("density_matrix", .density_matrix.restricted))
            call datafile%sbf%add_dataset(sbf_Dataset("molecular_orbitals", .molecular_orbitals.restricted))
            !DIE("Serializing these orbitals is not implemented")

         end

      end if

      call datafile%sbf%serialize

      datafile.close
       
    end if 

   end 

   deserialize(filename,do_groups)
   ! Read in data from sbf file
      self :: INOUT
      filename :: STR, IN
      do_groups :: BIN, optional, IN

      datafile :: DATAFILE
      tmp_str :: STR(len=:)@
      atomic_labels :: STR(len=1), dimension(:,:), allocatable
      atomic_numbers :: VEC{INT}@
      atomic_coordinates :: VEC{REAL}@
      charge_multiplicity :: VEC{INT}@
      tmp_mat :: MAT{REAL}@
      labels :: VEC{STR}@
      i, err :: INT
      success = 1 :: INT

    ! .:create

      datafile.open(filename)

      call datafile%sbf%deserialize

      call datafile%sbf%get("name", tmp_str, err)
      DIE_IF(err /= success, sbf_strerr(err))

      .name = tmp_str
      call datafile%sbf%get("basis_name", tmp_str, err)
      DIE_IF(err /= success, sbf_strerr(err))

      .basis_name = tmp_str

      call datafile%sbf%get("atomic_labels", atomic_labels, err)
      DIE_IF(err /= success, sbf_strerr(err))

      ! convert back
      allocate(labels(size(atomic_labels, 1)))
      do i = 1, size(atomic_labels, dim=1)
         labels(i) = trim(VEC{STR}:char_array_to_str(atomic_labels(i,:)))
      end do

      call datafile%sbf%get("atomic_numbers", atomic_numbers, err)
      DIE_IF(err /= success, sbf_strerr(err))
      .atom.create(size(atomic_numbers))

      call datafile%sbf%get("atomic_coordinates", atomic_coordinates, err)
      DIE_IF(err /= success, sbf_strerr(err))
      .atom.set_positions_from_vector(atomic_coordinates)

      .atom.set_atom_data_from_labels(labels)

      call datafile%sbf%get("charge_multiplicity", charge_multiplicity, err)
      if(err /= success) then
          stderr.show("Error reading charge_multiplicity from file, not setting:", sbf_strerr(err))
      else
          .:set_charge(charge_multiplicity(1))
          .:set_spin_multiplicity(charge_multiplicity(2))
      endif

      ! Finalize atom info/basis sets
      .:finalize_atoms(do_groups)

      call datafile%sbf%get("scf_kind", tmp_str, err)

      ! TODO CHECK FOR ERRORS
      if (err == success) then
         .scfdata.create
         .scfdata.set_output(FALSE)
         .density_matrix.create(.n_bf)
         .molecular_orbitals.create(.n_bf)

         select case (tmp_str)
         !select case (.scfdata.spinorbital_kind)

         case ("restricted")
            .scfdata.set_scf_kind("rhf")
            call datafile%sbf%get("density_matrix", tmp_mat, err)
            DIE_IF(err /= success, sbf_strerr(err))
            .density_matrix.create("restricted")
            .density_matrix.restricted = tmp_mat
            deallocate(tmp_mat)
            call datafile%sbf%get("molecular_orbitals", tmp_mat, err)
            DIE_IF(err /= success, sbf_strerr(err))
            .molecular_orbitals.create("restricted")
            .molecular_orbitals.restricted = tmp_mat
            deallocate(tmp_mat)

         case ("unrestricted")
            .scfdata.set_scf_kind("uhf")
            .density_matrix.create("unrestricted")
            call datafile%sbf%get("density_matrix.alpha", tmp_mat, err)
            DIE_IF(err /= success, sbf_strerr(err))
            .density_matrix.alpha= tmp_mat
            deallocate(tmp_mat)
            call datafile%sbf%get("density_matrix.beta", tmp_mat, err)
            DIE_IF(err /= success, sbf_strerr(err))
            .density_matrix.beta = tmp_mat
            deallocate(tmp_mat)
            .molecular_orbitals.create("unrestricted")
            call datafile%sbf%get("molecular_orbitals.alpha", tmp_mat, err)
            DIE_IF(err /= success, sbf_strerr(err))
            .molecular_orbitals.alpha = tmp_mat
            deallocate(tmp_mat)
            call datafile%sbf%get("molecular_orbitals.beta", tmp_mat, err)
            DIE_IF(err /= success, sbf_strerr(err))
            .molecular_orbitals.beta = tmp_mat
            deallocate(tmp_mat)

         case ("general")
            .scfdata.set_scf_kind("ghf")
            call datafile%sbf%get("density_matrix.general", tmp_mat, err)
            DIE_IF(err /= success, sbf_strerr(err))
            .density_matrix.create("general")
            .density_matrix.general = tmp_mat
            deallocate(tmp_mat)
            call datafile%sbf%get("molecular_orbitals.general", tmp_mat, err)
            DIE_IF(err /= success, sbf_strerr(err))
            .molecular_orbitals.create("general")
            .molecular_orbitals.general = tmp_mat
            deallocate(tmp_mat)

         case default
            DIE("Serializing these orbitals is not implemented")

         end

      else


      end if

      datafile.close

   end

   make_atomic_multipoles(od,oc,no_s)
   ! Make the atomic multipoles using the tanaka partitioing
      self :: PTR
      od, oc, no_s :: BIN, optional, IN

   ENSURE(.basis_info_made,"no basis info")
   ENSURE(.atom.associated,"no atom list")
   ENSURE(.density_matrix.allocated,"no density_matrix")
   ENSURE(.density_matrix.restricted.allocated,"no restricted density_matrix")
   ENSURE(NOT .use_spherical_basis,"not implememnted for spherical")

      off_diagonal,only_disconnected, no_s_functions :: BIN
      S,Sa,Sb,part :: MAT{REAL}*
      Ax,Ay,Az,Bx,By,Bz :: MAT{REAL}*
      Axx,Ayy,Azz,Axy,Axz,Ayz,Bxx,Byy,Bzz,Bxy,Bxz,Byz :: MAT{REAL}*
      Axxx,Ayyy,Azzz,Axxy,Axxz,Ayyx,Ayyz,Azzx,Azzy,Axyz :: MAT{REAL}*
      Bxxx,Byyy,Bzzz,Bxxy,Bxxz,Byyx,Byyz,Bzzx,Bzzy,Bxyz :: MAT{REAL}*
      q,fa,la,na,fb,lb,nb,atom_a,atom_b :: INT
      sh :: SHELL2
      fac :: REAL

      off_diagonal = FALSE
      only_disconnected = FALSE
      no_s_functions = FALSE
      if (present(od)) off_diagonal = od
      if (present(oc)) only_disconnected = oc
      if (present(no_s)) no_s_functions = no_s

      
      .charges.create(.n_atom)
      .dipoles.create(.n_atom, 3)
      .quadrupoles.create(.n_atom, 6)
      .octupoles.create(.n_atom, 10)
  

      .charges     = ZERO
      .dipoles     = ZERO
      .quadrupoles = ZERO
      .octupoles   = ZERO

      do q = 1,.n_shell_pairs

         .BASE:get_precomputed_shell_pair(sh,q,fa,la,na,fb,lb,nb,atom_a,atom_b)

         if (atom_a==atom_b) then

            if (no_s_functions AND sh.a.l == 0 AND sh.b.l == 0) cycle
            if (off_diagonal) cycle
            if (only_disconnected) cycle

            fac = TWO
            if (fa==fb) fac=ONE

            S.create(na,nb)
            Ax.create(na,nb); Ay.create(na,nb); Az.create(na,nb)
            Axx.create(na,nb); Ayy.create(na,nb); Azz.create(na,nb)
            Axy.create(na,nb); Axz.create(na,nb); Ayz.create(na,nb)

            Axxx.create(na,nb); Ayyy.create(na,nb); Azzz.create(na,nb)
            Axxy.create(na,nb); Axxz.create(na,nb); Ayyx.create(na,nb)
            Ayyz.create(na,nb); Azzx.create(na,nb); Azzy.create(na,nb)
            Axyz.create(na,nb); 

            sh.make_overlap_ints(S)
            sh.make_dipole_ints(Ax,Ay,Az,sh.a.position)
            sh.make_quadrupole_ints(Axx,Ayy,Azz,Axy,Axz,Ayz,sh.a.position)
            sh.make_octupole_ints(Axxx,Ayyy,Azzz,Axxy,Axxz,Ayyx,Ayyz,Azzx,Azzy,Axyz,sh.a.position)

            .charges(atom_a)       = .charges(atom_a)       + fac*sum(S*.density_matrix.restricted(fa:la,fb:lb))
            .dipoles(atom_a,1)     = .dipoles(atom_a,1)     + fac*sum(Ax*.density_matrix.restricted(fa:la,fb:lb))
            .dipoles(atom_a,2)     = .dipoles(atom_a,2)     + fac*sum(Ay*.density_matrix.restricted(fa:la,fb:lb))
            .dipoles(atom_a,3)     = .dipoles(atom_a,3)     + fac*sum(Az*.density_matrix.restricted(fa:la,fb:lb))
            .quadrupoles(atom_a,1) = .quadrupoles(atom_a,1) + fac*sum(Axx*.density_matrix.restricted(fa:la,fb:lb))
            .quadrupoles(atom_a,2) = .quadrupoles(atom_a,2) + fac*sum(Ayy*.density_matrix.restricted(fa:la,fb:lb))
            .quadrupoles(atom_a,3) = .quadrupoles(atom_a,3) + fac*sum(Azz*.density_matrix.restricted(fa:la,fb:lb))
            .quadrupoles(atom_a,4) = .quadrupoles(atom_a,4) + fac*sum(Axy*.density_matrix.restricted(fa:la,fb:lb))
            .quadrupoles(atom_a,5) = .quadrupoles(atom_a,5) + fac*sum(Axz*.density_matrix.restricted(fa:la,fb:lb))
            .quadrupoles(atom_a,6) = .quadrupoles(atom_a,6) + fac*sum(Ayz*.density_matrix.restricted(fa:la,fb:lb))

            .octupoles(atom_a, 1)  = .octupoles(atom_a, 1)  + fac*sum(Axxx*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_a, 2)  = .octupoles(atom_a, 2)  + fac*sum(Ayyy*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_a, 3)  = .octupoles(atom_a, 3)  + fac*sum(Azzz*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_a, 4)  = .octupoles(atom_a, 4)  + fac*sum(Axxy*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_a, 5)  = .octupoles(atom_a, 5)  + fac*sum(Axxz*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_a, 6)  = .octupoles(atom_a, 6)  + fac*sum(Ayyx*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_a, 7)  = .octupoles(atom_a, 7)  + fac*sum(Ayyz*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_a, 8)  = .octupoles(atom_a, 8)  + fac*sum(Azzx*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_a, 9)  = .octupoles(atom_a, 9)  + fac*sum(Azzy*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_a,10)  = .octupoles(atom_a,10)  + fac*sum(Axyz*.density_matrix.restricted(fa:la,fb:lb))

            S.destroy
            Ax.destroy; Ay.destroy; Az.destroy
            Axx.destroy; Ayy.destroy; Azz.destroy
            Axy.destroy; Axz.destroy; Ayz.destroy

            Axxx.destroy; Ayyy.destroy; Azzz.destroy
            Axxy.destroy; Axxz.destroy; Ayyx.destroy
            Ayyz.destroy; Azzx.destroy; Azzy.destroy
            Axyz.destroy; 

         else

            if(only_disconnected) then
               if (VEC{ATOM}:has_a_connection(atom_a,atom_b)) cycle
            end

            ! Partition factors
            part.create(sh.a.n_cc,sh.b.n_cc)
            sh.make_KM_partition(part)

            ! Get weighted overlap integrals
            Sa.create(na,nb); Sb.create(na,nb)

            sh.make_weighted_overlap_ints(Sa,Sb,part)

            .charges(atom_a) = .charges(atom_a) + TWO*sum(Sa*.density_matrix.restricted(fa:la,fb:lb))
            .charges(atom_b) = .charges(atom_b) + TWO*sum(Sb*.density_matrix.restricted(fa:la,fb:lb))

            Sb.destroy; Sa.destroy

            ! .dipoless
            Ax.create(na,nb); Ay.create(na,nb); Az.create(na,nb)
            Bx.create(na,nb); By.create(na,nb); Bz.create(na,nb)

            sh.make_weighted_dipole_ints(Ax,Ay,Az,Bx,By,Bz,part)

            .dipoles(atom_a,1) = .dipoles(atom_a,1) + TWO*sum(Ax*.density_matrix.restricted(fa:la,fb:lb))
            .dipoles(atom_a,2) = .dipoles(atom_a,2) + TWO*sum(Ay*.density_matrix.restricted(fa:la,fb:lb))
            .dipoles(atom_a,3) = .dipoles(atom_a,3) + TWO*sum(Az*.density_matrix.restricted(fa:la,fb:lb))

            .dipoles(atom_b,1) = .dipoles(atom_b,1) + TWO*sum(Bx*.density_matrix.restricted(fa:la,fb:lb))
            .dipoles(atom_b,2) = .dipoles(atom_b,2) + TWO*sum(By*.density_matrix.restricted(fa:la,fb:lb))
            .dipoles(atom_b,3) = .dipoles(atom_b,3) + TWO*sum(Bz*.density_matrix.restricted(fa:la,fb:lb))

            Ax.destroy; Ay.destroy; Az.destroy
            Bx.destroy; By.destroy; Bz.destroy

            ! .quadrupoless
            Axx.create(na,nb); Ayy.create(na,nb); Azz.create(na,nb)
            Axy.create(na,nb); Axz.create(na,nb); Ayz.create(na,nb)
            Bxx.create(na,nb); Byy.create(na,nb); Bzz.create(na,nb)
            Bxy.create(na,nb); Bxz.create(na,nb); Byz.create(na,nb)

            sh.make_weighted_quadrupole_ints(Axx,Ayy,Azz,Axy,Axz,Ayz &
                                            ,Bxx,Byy,Bzz,Bxy,Bxz,Byz, part)

            .quadrupoles(atom_a,1) = .quadrupoles(atom_a,1) + TWO*sum(Axx*.density_matrix.restricted(fa:la,fb:lb))
            .quadrupoles(atom_a,2) = .quadrupoles(atom_a,2) + TWO*sum(Ayy*.density_matrix.restricted(fa:la,fb:lb))
            .quadrupoles(atom_a,3) = .quadrupoles(atom_a,3) + TWO*sum(Azz*.density_matrix.restricted(fa:la,fb:lb))
            .quadrupoles(atom_a,4) = .quadrupoles(atom_a,4) + TWO*sum(Axy*.density_matrix.restricted(fa:la,fb:lb))
            .quadrupoles(atom_a,5) = .quadrupoles(atom_a,5) + TWO*sum(Axz*.density_matrix.restricted(fa:la,fb:lb))
            .quadrupoles(atom_a,6) = .quadrupoles(atom_a,6) + TWO*sum(Ayz*.density_matrix.restricted(fa:la,fb:lb))

            .quadrupoles(atom_b,1) = .quadrupoles(atom_b,1) + TWO*sum(Bxx*.density_matrix.restricted(fa:la,fb:lb))
            .quadrupoles(atom_b,2) = .quadrupoles(atom_b,2) + TWO*sum(Byy*.density_matrix.restricted(fa:la,fb:lb))
            .quadrupoles(atom_b,3) = .quadrupoles(atom_b,3) + TWO*sum(Bzz*.density_matrix.restricted(fa:la,fb:lb))
            .quadrupoles(atom_b,4) = .quadrupoles(atom_b,4) + TWO*sum(Bxy*.density_matrix.restricted(fa:la,fb:lb))
            .quadrupoles(atom_b,5) = .quadrupoles(atom_b,5) + TWO*sum(Bxz*.density_matrix.restricted(fa:la,fb:lb))
            .quadrupoles(atom_b,6) = .quadrupoles(atom_b,6) + TWO*sum(Byz*.density_matrix.restricted(fa:la,fb:lb))

            Axx.destroy; Ayy.destroy; Azz.destroy
            Axy.destroy; Axz.destroy; Ayz.destroy
            Bxx.destroy; Byy.destroy; Bzz.destroy
            Bxy.destroy; Bxz.destroy; Byz.destroy

            ! .octupoless
            Axxx.create(na,nb); Ayyy.create(na,nb); Azzz.create(na,nb)
            Axxy.create(na,nb); Axxz.create(na,nb); Ayyx.create(na,nb)
            Ayyz.create(na,nb); Azzx.create(na,nb); Azzy.create(na,nb)
            Axyz.create(na,nb); 
            Bxxx.create(na,nb); Byyy.create(na,nb); Bzzz.create(na,nb)
            Bxxy.create(na,nb); Bxxz.create(na,nb); Byyx.create(na,nb)
            Byyz.create(na,nb); Bzzx.create(na,nb); Bzzy.create(na,nb)
            Bxyz.create(na,nb); 

            sh.make_weighted_octupole_ints(Axxx,Ayyy,Azzz,Axxy,Axxz,Ayyx,Ayyz,Azzx,Azzy,Axyz & 
                                          ,Bxxx,Byyy,Bzzz,Bxxy,Bxxz,Byyx,Byyz,Bzzx,Bzzy,Bxyz,part)

            .octupoles(atom_a, 1) = .octupoles(atom_a, 1) + TWO*sum(Axxx*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_a, 2) = .octupoles(atom_a, 2) + TWO*sum(Ayyy*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_a, 3) = .octupoles(atom_a, 3) + TWO*sum(Azzz*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_a, 4) = .octupoles(atom_a, 4) + TWO*sum(Axxy*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_a, 5) = .octupoles(atom_a, 5) + TWO*sum(Axxz*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_a, 6) = .octupoles(atom_a, 6) + TWO*sum(Ayyx*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_a, 7) = .octupoles(atom_a, 7) + TWO*sum(Ayyz*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_a, 8) = .octupoles(atom_a, 8) + TWO*sum(Azzx*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_a, 9) = .octupoles(atom_a, 9) + TWO*sum(Azzy*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_a,10) = .octupoles(atom_a,10) + TWO*sum(Axyz*.density_matrix.restricted(fa:la,fb:lb))

            .octupoles(atom_b, 1) = .octupoles(atom_b, 1) + TWO*sum(Bxxx*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_b, 2) = .octupoles(atom_b, 2) + TWO*sum(Byyy*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_b, 3) = .octupoles(atom_b, 3) + TWO*sum(Bzzz*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_b, 4) = .octupoles(atom_b, 4) + TWO*sum(Bxxy*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_b, 5) = .octupoles(atom_b, 5) + TWO*sum(Bxxz*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_b, 6) = .octupoles(atom_b, 6) + TWO*sum(Byyx*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_b, 7) = .octupoles(atom_b, 7) + TWO*sum(Byyz*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_b, 8) = .octupoles(atom_b, 8) + TWO*sum(Bzzx*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_b, 9) = .octupoles(atom_b, 9) + TWO*sum(Bzzy*.density_matrix.restricted(fa:la,fb:lb))
            .octupoles(atom_b,10) = .octupoles(atom_b,10) + TWO*sum(Bxyz*.density_matrix.restricted(fa:la,fb:lb))

            Axxx.destroy; Ayyy.destroy; Azzz.destroy
            Axxy.destroy; Axxz.destroy; Ayyx.destroy
            Ayyz.destroy; Azzx.destroy; Azzy.destroy
            Axyz.destroy; 
            Bxxx.destroy; Byyy.destroy; Bzzz.destroy
            Bxxy.destroy; Bxxz.destroy; Byyx.destroy
            Byyz.destroy; Bzzx.destroy; Bzzy.destroy
            Bxyz.destroy; 

            part.destroy

         end

         sh.destroy_ptr_part

      end

      ! Electron .charges
      .dipoles     = -ONE * .dipoles
      .quadrupoles = -ONE * .quadrupoles
      .octupoles   = -ONE * .octupoles

   end

   make_atomic_multipoles(charge,dipole,quadrupole,octupole,od,oc,no_s)
   ! Make the atomic multipoles using the tanaka partitioing
      self :: IN
      charge :: VEC{REAL}, OUT
      dipole, quadrupole, octupole :: MAT{REAL}, OUT
      od, oc, no_s :: BIN, optional, IN

   ENSURE(.basis_info_made,"no basis info")
   ENSURE(.atom.associated,"no atom list")
   ENSURE(.density_matrix.allocated,"no density_matrix")
   ENSURE(.density_matrix.restricted.allocated,"no restricted density_matrix")
   ENSURE(NOT .use_spherical_basis,"not implememnted for spherical")

      off_diagonal,only_disconnected, no_s_functions :: BIN
      S,Sa,Sb,part :: MAT{REAL}*
      Ax,Ay,Az,Bx,By,Bz :: MAT{REAL}*
      Axx,Ayy,Azz,Axy,Axz,Ayz,Bxx,Byy,Bzz,Bxy,Bxz,Byz :: MAT{REAL}*
      Axxx,Ayyy,Azzz,Axxy,Axxz,Ayyx,Ayyz,Azzx,Azzy,Axyz :: MAT{REAL}*
      Bxxx,Byyy,Bzzz,Bxxy,Bxxz,Byyx,Byyz,Bzzx,Bzzy,Bxyz :: MAT{REAL}*
      q,fa,la,na,fb,lb,nb,atom_a,atom_b :: INT
      sh :: SHELL2
      fac :: REAL

      off_diagonal = FALSE
      only_disconnected = FALSE
      no_s_functions = FALSE
      if (present(od)) off_diagonal = od
      if (present(oc)) only_disconnected = oc
      if (present(no_s)) no_s_functions = no_s

      
      charge     = ZERO
      dipole     = ZERO
      quadrupole = ZERO
      octupole   = ZERO

      do q = 1,.n_shell_pairs

         .BASE:get_precomputed_shell_pair(sh,q,fa,la,na,fb,lb,nb,atom_a,atom_b)

         if (atom_a==atom_b) then

            if (no_s_functions AND sh.a.l == 0 AND sh.b.l == 0) cycle
            if (off_diagonal) cycle
            if (only_disconnected) cycle

            fac = TWO
            if (fa==fb) fac=ONE

            S.create(na,nb)
            Ax.create(na,nb); Ay.create(na,nb); Az.create(na,nb)
            Axx.create(na,nb); Ayy.create(na,nb); Azz.create(na,nb)
            Axy.create(na,nb); Axz.create(na,nb); Ayz.create(na,nb)

            Axxx.create(na,nb); Ayyy.create(na,nb); Azzz.create(na,nb)
            Axxy.create(na,nb); Axxz.create(na,nb); Ayyx.create(na,nb)
            Ayyz.create(na,nb); Azzx.create(na,nb); Azzy.create(na,nb)
            Axyz.create(na,nb); 

            sh.make_overlap_ints(S)
            sh.make_dipole_ints(Ax,Ay,Az,sh.a.position)
            sh.make_quadrupole_ints(Axx,Ayy,Azz,Axy,Axz,Ayz,sh.a.position)
            sh.make_octupole_ints(Axxx,Ayyy,Azzz,Axxy,Axxz,Ayyx,Ayyz,Azzx,Azzy,Axyz,sh.a.position)

            charge(atom_a)       = charge(atom_a)       + fac*sum(S*.density_matrix.restricted(fa:la,fb:lb))
            dipole(atom_a,1)     = dipole(atom_a,1)     + fac*sum(Ax*.density_matrix.restricted(fa:la,fb:lb))
            dipole(atom_a,2)     = dipole(atom_a,2)     + fac*sum(Ay*.density_matrix.restricted(fa:la,fb:lb))
            dipole(atom_a,3)     = dipole(atom_a,3)     + fac*sum(Az*.density_matrix.restricted(fa:la,fb:lb))
            quadrupole(atom_a,1) = quadrupole(atom_a,1) + fac*sum(Axx*.density_matrix.restricted(fa:la,fb:lb))
            quadrupole(atom_a,2) = quadrupole(atom_a,2) + fac*sum(Ayy*.density_matrix.restricted(fa:la,fb:lb))
            quadrupole(atom_a,3) = quadrupole(atom_a,3) + fac*sum(Azz*.density_matrix.restricted(fa:la,fb:lb))
            quadrupole(atom_a,4) = quadrupole(atom_a,4) + fac*sum(Axy*.density_matrix.restricted(fa:la,fb:lb))
            quadrupole(atom_a,5) = quadrupole(atom_a,5) + fac*sum(Axz*.density_matrix.restricted(fa:la,fb:lb))
            quadrupole(atom_a,6) = quadrupole(atom_a,6) + fac*sum(Ayz*.density_matrix.restricted(fa:la,fb:lb))

            octupole(atom_a, 1)  = octupole(atom_a, 1)  + fac*sum(Axxx*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_a, 2)  = octupole(atom_a, 2)  + fac*sum(Ayyy*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_a, 3)  = octupole(atom_a, 3)  + fac*sum(Azzz*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_a, 4)  = octupole(atom_a, 4)  + fac*sum(Axxy*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_a, 5)  = octupole(atom_a, 5)  + fac*sum(Axxz*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_a, 6)  = octupole(atom_a, 6)  + fac*sum(Ayyx*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_a, 7)  = octupole(atom_a, 7)  + fac*sum(Ayyz*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_a, 8)  = octupole(atom_a, 8)  + fac*sum(Azzx*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_a, 9)  = octupole(atom_a, 9)  + fac*sum(Azzy*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_a,10)  = octupole(atom_a,10)  + fac*sum(Axyz*.density_matrix.restricted(fa:la,fb:lb))

            S.destroy
            Ax.destroy; Ay.destroy; Az.destroy
            Axx.destroy; Ayy.destroy; Azz.destroy
            Axy.destroy; Axz.destroy; Ayz.destroy

            Axxx.destroy; Ayyy.destroy; Azzz.destroy
            Axxy.destroy; Axxz.destroy; Ayyx.destroy
            Ayyz.destroy; Azzx.destroy; Azzy.destroy
            Axyz.destroy; 

         else

            if(only_disconnected) then
               if (VEC{ATOM}:has_a_connection(atom_a,atom_b)) cycle
            end

            ! Partition factors
            part.create(sh.a.n_cc,sh.b.n_cc)
            sh.make_KM_partition(part)

            ! Get weighted overlap integrals
            Sa.create(na,nb); Sb.create(na,nb)

            sh.make_weighted_overlap_ints(Sa,Sb,part)

            charge(atom_a) = charge(atom_a) + TWO*sum(Sa*.density_matrix.restricted(fa:la,fb:lb))
            charge(atom_b) = charge(atom_b) + TWO*sum(Sb*.density_matrix.restricted(fa:la,fb:lb))

            Sb.destroy; Sa.destroy

            ! Dipoles
            Ax.create(na,nb); Ay.create(na,nb); Az.create(na,nb)
            Bx.create(na,nb); By.create(na,nb); Bz.create(na,nb)

            sh.make_weighted_dipole_ints(Ax,Ay,Az,Bx,By,Bz,part)

            dipole(atom_a,1) = dipole(atom_a,1) + TWO*sum(Ax*.density_matrix.restricted(fa:la,fb:lb))
            dipole(atom_a,2) = dipole(atom_a,2) + TWO*sum(Ay*.density_matrix.restricted(fa:la,fb:lb))
            dipole(atom_a,3) = dipole(atom_a,3) + TWO*sum(Az*.density_matrix.restricted(fa:la,fb:lb))

            dipole(atom_b,1) = dipole(atom_b,1) + TWO*sum(Bx*.density_matrix.restricted(fa:la,fb:lb))
            dipole(atom_b,2) = dipole(atom_b,2) + TWO*sum(By*.density_matrix.restricted(fa:la,fb:lb))
            dipole(atom_b,3) = dipole(atom_b,3) + TWO*sum(Bz*.density_matrix.restricted(fa:la,fb:lb))

            Ax.destroy; Ay.destroy; Az.destroy
            Bx.destroy; By.destroy; Bz.destroy

            ! Quadrupoles
            Axx.create(na,nb); Ayy.create(na,nb); Azz.create(na,nb)
            Axy.create(na,nb); Axz.create(na,nb); Ayz.create(na,nb)
            Bxx.create(na,nb); Byy.create(na,nb); Bzz.create(na,nb)
            Bxy.create(na,nb); Bxz.create(na,nb); Byz.create(na,nb)

            sh.make_weighted_quadrupole_ints(Axx,Ayy,Azz,Axy,Axz,Ayz &
                                            ,Bxx,Byy,Bzz,Bxy,Bxz,Byz, part)

            quadrupole(atom_a,1) = quadrupole(atom_a,1) + TWO*sum(Axx*.density_matrix.restricted(fa:la,fb:lb))
            quadrupole(atom_a,2) = quadrupole(atom_a,2) + TWO*sum(Ayy*.density_matrix.restricted(fa:la,fb:lb))
            quadrupole(atom_a,3) = quadrupole(atom_a,3) + TWO*sum(Azz*.density_matrix.restricted(fa:la,fb:lb))
            quadrupole(atom_a,4) = quadrupole(atom_a,4) + TWO*sum(Axy*.density_matrix.restricted(fa:la,fb:lb))
            quadrupole(atom_a,5) = quadrupole(atom_a,5) + TWO*sum(Axz*.density_matrix.restricted(fa:la,fb:lb))
            quadrupole(atom_a,6) = quadrupole(atom_a,6) + TWO*sum(Ayz*.density_matrix.restricted(fa:la,fb:lb))

            quadrupole(atom_b,1) = quadrupole(atom_b,1) + TWO*sum(Bxx*.density_matrix.restricted(fa:la,fb:lb))
            quadrupole(atom_b,2) = quadrupole(atom_b,2) + TWO*sum(Byy*.density_matrix.restricted(fa:la,fb:lb))
            quadrupole(atom_b,3) = quadrupole(atom_b,3) + TWO*sum(Bzz*.density_matrix.restricted(fa:la,fb:lb))
            quadrupole(atom_b,4) = quadrupole(atom_b,4) + TWO*sum(Bxy*.density_matrix.restricted(fa:la,fb:lb))
            quadrupole(atom_b,5) = quadrupole(atom_b,5) + TWO*sum(Bxz*.density_matrix.restricted(fa:la,fb:lb))
            quadrupole(atom_b,6) = quadrupole(atom_b,6) + TWO*sum(Byz*.density_matrix.restricted(fa:la,fb:lb))

            Axx.destroy; Ayy.destroy; Azz.destroy
            Axy.destroy; Axz.destroy; Ayz.destroy
            Bxx.destroy; Byy.destroy; Bzz.destroy
            Bxy.destroy; Bxz.destroy; Byz.destroy

            ! Octupoles
            Axxx.create(na,nb); Ayyy.create(na,nb); Azzz.create(na,nb)
            Axxy.create(na,nb); Axxz.create(na,nb); Ayyx.create(na,nb)
            Ayyz.create(na,nb); Azzx.create(na,nb); Azzy.create(na,nb)
            Axyz.create(na,nb); 
            Bxxx.create(na,nb); Byyy.create(na,nb); Bzzz.create(na,nb)
            Bxxy.create(na,nb); Bxxz.create(na,nb); Byyx.create(na,nb)
            Byyz.create(na,nb); Bzzx.create(na,nb); Bzzy.create(na,nb)
            Bxyz.create(na,nb); 

            sh.make_weighted_octupole_ints(Axxx,Ayyy,Azzz,Axxy,Axxz,Ayyx,Ayyz,Azzx,Azzy,Axyz & 
                                          ,Bxxx,Byyy,Bzzz,Bxxy,Bxxz,Byyx,Byyz,Bzzx,Bzzy,Bxyz,part)

            octupole(atom_a, 1) = octupole(atom_a, 1) + TWO*sum(Axxx*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_a, 2) = octupole(atom_a, 2) + TWO*sum(Ayyy*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_a, 3) = octupole(atom_a, 3) + TWO*sum(Azzz*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_a, 4) = octupole(atom_a, 4) + TWO*sum(Axxy*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_a, 5) = octupole(atom_a, 5) + TWO*sum(Axxz*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_a, 6) = octupole(atom_a, 6) + TWO*sum(Ayyx*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_a, 7) = octupole(atom_a, 7) + TWO*sum(Ayyz*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_a, 8) = octupole(atom_a, 8) + TWO*sum(Azzx*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_a, 9) = octupole(atom_a, 9) + TWO*sum(Azzy*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_a,10) = octupole(atom_a,10) + TWO*sum(Axyz*.density_matrix.restricted(fa:la,fb:lb))

            octupole(atom_b, 1) = octupole(atom_b, 1) + TWO*sum(Bxxx*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_b, 2) = octupole(atom_b, 2) + TWO*sum(Byyy*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_b, 3) = octupole(atom_b, 3) + TWO*sum(Bzzz*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_b, 4) = octupole(atom_b, 4) + TWO*sum(Bxxy*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_b, 5) = octupole(atom_b, 5) + TWO*sum(Bxxz*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_b, 6) = octupole(atom_b, 6) + TWO*sum(Byyx*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_b, 7) = octupole(atom_b, 7) + TWO*sum(Byyz*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_b, 8) = octupole(atom_b, 8) + TWO*sum(Bzzx*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_b, 9) = octupole(atom_b, 9) + TWO*sum(Bzzy*.density_matrix.restricted(fa:la,fb:lb))
            octupole(atom_b,10) = octupole(atom_b,10) + TWO*sum(Bxyz*.density_matrix.restricted(fa:la,fb:lb))

            Axxx.destroy; Ayyy.destroy; Azzz.destroy
            Axxy.destroy; Axxz.destroy; Ayyx.destroy
            Ayyz.destroy; Azzx.destroy; Azzy.destroy
            Axyz.destroy; 
            Bxxx.destroy; Byyy.destroy; Bzzz.destroy
            Bxxy.destroy; Bxxz.destroy; Byyx.destroy
            Byyz.destroy; Bzzx.destroy; Bzzy.destroy
            Bxyz.destroy; 

            part.destroy

         end

         sh.destroy_ptr_part

      end

      ! Electron charge
      dipole     = -ONE * dipole
      quadrupole = -ONE * quadrupole
      octupole   = -ONE * octupole

   end


end
