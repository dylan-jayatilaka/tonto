      .occupation_numbers.destroy
      .occupation_numbers.create(.n_bf,"restricted")

      V.create(.n_bf,.n_bf)
      O.create(.n_bf,.n_bf)

      .:make_r_overlap_sqrt(O)

      X.create(.n_bf,.n_bf)
