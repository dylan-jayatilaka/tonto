      .spin_multiplicity   = 1
      .n_e                 = 0
      .n_a                 = 0
      .n_b                 = 0
      .E_field             = ZERO
      .B_field             = ZERO
      .gauge_origin        = ZERO

      .n_atom              = 0
      .atom_info_made      = FALSE
      .n_atom_kind         = 0

      .n_bf                = 0
      .basis_name          = " "
      .n_basis             = 0
      .basis_l_max         = -1
      .use_spherical_basis = FALSE
      .n_bf                = 0
      .n_prim              = 0
      .slaterbasis_name    = " "
