      .n_bf          = .atom.no_of_basis_functions
      .n_prim        = .atom.no_of_primitives
      .n_shell       = .atom.no_of_shells
      .n_shell_pairs = .atom.no_of_shell_pairs

      ! Make shell-basis-function limts, etc. (keaky)
