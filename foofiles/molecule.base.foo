         S(:,:,1) = IM(  .density_matrix.general_complex.alpha_beta)  &
                  - IM(  .density_matrix.general_complex.beta_alpha)
         S(:,:,2) = IM(I*.density_matrix.general_complex.alpha_beta)  &
                  - IM(I*.density_matrix.general_complex.beta_alpha)
         S(:,:,3) = IM(  .density_matrix.general_complex.alpha_alpha) &
                  - IM(  .density_matrix.general_complex.beta_beta)

      case default
         DIE("unknown kind, "//trim(genre))

      end

      ! Factor 1/2
      S = HALF*S

   end

   make_scf_density_matrix(n,nb)
   ! Make the density matrix from orbital "n" of the molecular orbitals.
   ! If present, orbital "nb" of the beta molecular orbitals is used.
   ! If either index is not an occupied MO, the density is set to zero.
      self :: INOUT
      n :: INT, IN
      nb :: INT, optional, IN

   ENSURE(.basis_info_made, "no basis info")
   ENSURE(.molecular_orbitals.allocated, "no molecular orbitals")

      genre :: STR
      arch :: ARCHIVE
      m :: INT
      uhf :: BIN

      uhf = .scfdata.scf_kind.includes("unrestricted")
      WARN_IF(present(nb) AND (NOT uhf), "nb ignored")

      m = n
      if (present(nb)) m = nb

      genre = .scfdata.spinorbital_kind
      .density_matrix.destroy(genre)
      .density_matrix.create(genre)

      select case (.scfdata.scf_kind)

      case ("rhf","rks","xray_rhf","xray_rks","noninteracting-group-rhf")

         ENSURE(.spin_multiplicity==1,"Not a singlet state: "//trim(.BASE:chemical_formula)//", mult="//trim(.spin_multiplicity.to_str))

         if (n>0 AND n<=.n_a) then
            .density_matrix.restricted.to_scaled_product_of(.molecular_orbitals.restricted(:,n:n),.molecular_orbitals.restricted(:,n:n),fac=TWO,transpose_b=TRUE)
         else
            .density_matrix.restricted = ZERO
         end

      case ("rohf")

         if (n>0 AND n<=.n_b) then
            .density_matrix.alpha.to_product_of(.molecular_orbitals.alpha(:,n:n),.molecular_orbitals.alpha(:,n:n),transpose_b=TRUE)
            .density_matrix.beta.to_product_of(.molecular_orbitals.beta(:,n:n),.molecular_orbitals.beta(:,n:n),transpose_b=TRUE)
         else if (n>nb AND n<=.n_a) then
            .molecular_orbitals.alpha(:,n:n) = .molecular_orbitals.restricted(:,n:n)
            .density_matrix.alpha.to_product_of(.molecular_orbitals.alpha(:,n:n),.molecular_orbitals.alpha(:,n:n),transpose_b=TRUE)
            .density_matrix.alpha = ZERO
            .density_matrix.beta = ZERO
         end

      case ("uhf","uks","xray_uhf","xray_uks", &
            "pnd_uhf","pnd_uks","xray_pnd_uhf","xray_pnd_uks")


         if (n>0 AND n<=.n_a) then
            .density_matrix.alpha.to_product_of(.molecular_orbitals.alpha(:,n:n),.molecular_orbitals.alpha(:,n:n),transpose_b=TRUE)
         else
            .density_matrix.alpha = ZERO
         end

         if (m>0 AND m<=.n_b) then
            .density_matrix.beta.to_product_of(.molecular_orbitals.beta(:,m:m),.molecular_orbitals.beta(:,m:m),transpose_b=TRUE)
         else
            .density_matrix.beta = ZERO
         end

      case ("ghf", "no_ghf")

         if (n>0 AND n<=.n_e) then
            .density_matrix.general.to_product_of(.molecular_orbitals.general(:,n:n),.molecular_orbitals.general(:,n:n),transpose_b=TRUE)
         else
            .density_matrix.general = ZERO
         end

      case ("rchf")

         if (n>0 AND n<=.n_a) then
            .density_matrix.restricted_complex.to_scaled_product_of(.molecular_orbitals.restricted_complex(:,n:n),.molecular_orbitals.restricted_complex(:,n:n),fac=TWO,dagger_b=TRUE)
         else
            .density_matrix.restricted_complex = ZERO
         end

      case ("uchf")

         if (n>0 AND n<=.n_a) then
            .density_matrix.alpha_complex.to_product_of(.molecular_orbitals.alpha_complex(:,n:n),.molecular_orbitals.alpha_complex(:,n:n),dagger_b=TRUE)
         else
            .density_matrix.alpha_complex = ZERO
         end

         if (m>0 AND m<=.n_b) then
            .density_matrix.beta_complex.to_product_of(.molecular_orbitals.beta_complex(:,m:m),.molecular_orbitals.beta_complex(:,m:m),dagger_b=TRUE)
         else
            .density_matrix.beta_complex = ZERO
         end

      case ("gchf","xray_gchf")

         if (n>0 AND n<=.n_e) then
            .density_matrix.general_complex.to_product_of(.molecular_orbitals.general_complex(:,n:n),.molecular_orbitals.general_complex(:,n:n),dagger_b=TRUE)
         else
            .density_matrix.general_complex = ZERO
         end

      case default;
         DIE("unknown kind, "//trim(.scfdata.scf_kind))

      end

      ! Archive
      arch.set(.name,"density_matrix",genre)
      arch.write(.density_matrix)

   end

