   finalize_atom_groups ::: leaky
   ! Finalize the .atom_group information
      self :: INOUT

      if (.atom.disassociated) then
        
         ! Make .atom()'s and .atom_group().mol's from files
         if (.atom_group.has_wavefunction_files_ptr) then
            .:make_groups_from_wfn_files
         end

      else if (.atom.has_cartesian_axes) then

         ! We have .atom()'s
         if (.crystal.use_Ryde_capping) then
        
            ! Ulf Ryde's capped residues
            ! WARNING: .atom_groups list only (parent) uncapped atoms
            .:make_Ryde_capped_groups
        
         else if (.atom_group.has_atom_indices_ptr) then
         
            ! User set atom_indices for each atom_group 
            .:make_atom_group_mols
        
         else 

            ! Make .atom_group's from connection table
            .:make_connected_atom_groups
            .:make_atom_group_mols

         end

      end

   end

   make_connected_atom_groups ::: leaky
   ! Make .atom_group's from the connection table.
      self :: INOUT

      n_group,g :: INT

      ! Set group indices, if possible
      .atom.set_connected_groups
      
      if (.atom.has_groups) then
      
         ! Each .atom() had a .group index
         n_group = .atom.no_of_groups
         if (.atom_group.disassociated) then
            .atom_group.create(n_group)
         else
            DIE_IF(n_group/=.atom_group.dim,"inconsistent .atom_group size")
         end
      
         ! Make groups from group index
         do g = 1,.atom_group.dim
            .atom_group(g).atom_index => .atom(:).group.indices_of_elements_matching(g)
            .atom_group(g).n_atoms     = .atom_group(g).atom_index.dim
         end
      
         ! Set charges
         if (.atom_group_charges.associated) then
            DIE_IF(.atom_group_charges.dim/=.atom_group.dim,"Wrong size, .atom_group_charges")
            do g = 1,.atom_group.dim
               .atom_group(g).set_charge(.atom_group_charges(g))
            end
         end

      end

   end

   make_atom_group_mols ::: private, leaky
   ! Make the ".atom_group.mol" molecules
      self :: INOUT

   DIE_IF(.atom.disassociated,"no atoms!")

      mol :: MOLECULE*
      g :: INT

      if (.atom_group.disassociated) return

      ! Regenerate molecules
      .:destroy_atom_group_mols

      ! Check is indices are sensible
      .atom_group.finalize(.atom)
      
      ! Create each group from "self"
      do g = 1,.atom_group.dim
      
         ! Create mol & define it
         ! ... including mol.crystal part
         mol.:create
         self.:make_molecule_from_atom_group(g,mol)
      
         ! Read the MOs & make the density
         ! This won't work until Tonto can read in its own dump
         if (.atom_group(g).MO_file_name/=" ") then
             mol.:read_archive("molecular_orbitals","restricted")
             mol.:make_scf_density_matrix
         end
      
         ! Rotate and translate mol
         if (.atom_group(g).transform_group) then
            mol.:rotate_by(.atom_group(g).rotation_matrix)
            mol.atom.translate_by(.atom_group(g).shift)
         end
         mol.atom.set_group_to(g)
         mol.name = .atom_group(g).name
      
         ! Add it to atom_group
         .atom_group(g).mol => mol
      
      end

      ! Update group info from self
      .:update_group_info

   end

   update_group_info ::: leaky
   ! Update .atom_group information (ANOs, crystal, SCF guess ...)
   ! WARNING: atom_group(:).mol.crystal's are clones of .crystal
      self :: INOUT

   ENSURE(.atom.associated,"atom groups must be defined after atoms=")
   ENSURE(.atom_group.has_all_mols,"atom_group has missing mols")

      mol :: MOLECULE*
      g :: INT

      if (.scfdata.disassociated) then
         .:set_scf_defaults
         .scfdata.set_output(FALSE)
      end

      do g = 1,.atom_group.dim

         mol => .atom_group(g).mol
         ENSURE(mol.associated,"atom_group molecule "//trim(g.to_str)//" does not exist")

         ! Don't remake ANO data if there
         mol.:set_ANOs_from(self)

         ! Assign the crystal & gegenerate unit cell atoms
         if (.crystal.associated) then
            mol.crystal.create_minimal_copy(.crystal)
            mol.crystal.make_fragment_data(mol.atom,assign_atom=FALSE)
         end

         ! Set guess-type SCF defaults
         if (NOT .atom_group(g).spin_multiplicity_set) then 
            mol.:set_scf_guess_defaults_from(.scfdata)
         end

      end

   end

! WFN file stuff

   create_from_mols(mol_A, mol_B) ::: leaky
   ! Create a molecule by merging "mol_A" and "mol_B"
      self :: PTR
      mol_A, mol_B :: MOLECULE*

      i, n_A, n_B :: INT

      .:create
      .:set_defaults

      mol_A.:make_scf_density_matrix
      mol_B.:make_scf_density_matrix

      n_A = mol_A.atom.dim
      n_B = mol_B.atom.dim

      .atom.create_copy(mol_A.atom)
      .atom.append(mol_B.atom)
      .atom_group.create(2)
      .atom_group(1).set_atom(mol_A.atom)
      .atom_group(1).atom_index.create_copy([ (i, i=1,n_A) ])
      .atom_group(1).mol => mol_A

      .atom_group(2).set_atom(mol_B.atom)
      .atom_group(2).atom_index.create_copy([ (i, i=n_A + 1,n_A + n_B) ])
      .atom_group(2).mol => mol_B

      .:join_atom_groups

      if (.atom_group.has_all_mols) then
         .:update_group_info
      end

   end

   read_wfn_file(filename,do_groups) ::: leaky
   ! Open and read a file named "name", either a gaussian .FChk file
   ! or an .sbf file.
      self :: INOUT
      filename :: STR, IN
      do_groups :: BIN, optional, IN

      if (filename.ends_in("fchk", case_sensitive=FALSE)) then
         .:read_g09_FChk_file(filename,do_groups)
      else if (filename.ends_in("sbf", case_sensitive=FALSE)) then
         .:deserialize(filename,do_groups)
      else
         DIE("unknown file format for file '"//trim(filename)//"'")
      end

   end

   make_groups_from_wfn_files ::: private, leaky
   ! Make the atom groups by read in Gaussian Fchk files in they are defined for the groups
      self :: INOUT

   DIE_IF(.atom.associated,"atoms already exists!")
   DIE_IF(.atom_group.disassociated,"no atom groups")
   DIE_IF(NOT .atom_group.has_wavefunction_files,"no wavefunction files")

      mol :: MOLECULE*
      wavefunction_file :: STR
      g :: INT

      do g = 1,.atom_group.dim

         ! Create the mol
         mol.:create

         ! Define mol from FChk or sbf file
         wavefunction_file = .atom_group(g).wavefunction_file_name
         mol.:read_wfn_file(wavefunction_file,do_groups=FALSE)

         ! Set kind of SCF
         ! ... this seems always to be UHF?
         if (mol.scfdata.is_unrestricted) then
            if(.scfdata.disassociated) then
               .scfdata.create
               .scfdata.set_defaults
            end
            .scfdata.set_scf_kind("uhf")
         end

         ! Rotate and translate it
         mol.:rotate_by(.atom_group(g).rotation_matrix)
         mol.atom.translate_by(.atom_group(g).shift)
         mol.atom.set_group_to(g)

         ! Add it to atom_group
         .atom_group(g).mol => mol

      end

      ! Define .atom list
      .:join_atom_groups

      ! Update group info from self
      if (.atom_group.has_all_mols) then
         .:update_group_info
      end

   end

   join_atom_groups ::: leaky, private
   ! Delete any existing .atom and .basis and set to the concatenation
   ! of the molecules in .atom_group
      self :: INOUT

      mol :: MOLECULE*
      g, charge, unpaired_electrons :: INT

      if (.atom.associated)  .atom.destroy
      if (.basis.associated) .basis.destroy

      charge = 0
      unpaired_electrons = 0

      ! Append remaining ...
      do g = 1,.atom_group.dim

         mol => .atom_group(g).mol

         DIE_IF(mol.disassociated,"no mol "//trim(g.to_str))
         DIE_IF(mol.basis.disassociated,"no basis on mol "//trim(g.to_str))
         DIE_IF(mol.atom.disassociated,"no atom list in mol "//trim(g.to_str))

         charge = charge + mol.charge
         unpaired_electrons = unpaired_electrons + mol.spin_multiplicity - 1 

         if (g==1) then

            ! Initial atom/basis list
            .name = .atom_group(g).name
            .basis.create_copy(mol.basis)
            .atom.create_copy(mol.atom)

         else

            ! Append basis & atoms
            .name = trim(.name) // "_" // trim(.atom_group(g).name)
            .basis.append(mol.basis)
            .atom.append(mol.atom)

         end

      end

      ! Unresolve atomic bases
      .atom.nullify_bases

      ! Uniquify basis
      .basis.remove_repetitions

      ! Finally: set atom info & basis info (leaky)
      .:set_charge(charge)
      .:set_spin_multiplicity(unpaired_electrons + 1)

      ! Finalise atom info
      .:finalize_atoms(do_groups=FALSE)

   end

