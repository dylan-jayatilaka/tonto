   assign_NOs_to_MOs ::: leaky, PURE
   ! Copy the natural orbitals to be the molecular_orbitals
   ! The occupation numbers are the MO ones.
      self :: INOUT

   ENSURE(.molecular_orbitals.allocated, "no molecular orbitals")
   ENSURE(.molecular_orbitals.has_any_genre, "no molecular orbitals")

      ON_kind :: STR
      mol :: MOLECULE*
      g :: INT

      if (.molecular_orbitals.allocated) then

         ON_kind = .molecular_orbitals.occupation_spinorbital_kind

         .natural_orbitals.destroy
         .natural_orbitals = .molecular_orbitals

         .occupation_numbers.destroy
         .occupation_numbers.create(.n_bf,ON_kind)

         .occupation_numbers.set_to_zero
         .:set_scf_occupations(ON_kind)

      end

      if (.atom_group.associated) then
      if (.atom_group(1).mol.associated) then
         
         do g = 1,.atom_group.dim

            mol => .atom_group(g).mol

            if (mol.molecular_orbitals.deallocated) cycle

            ON_kind = .molecular_orbitals.occupation_spinorbital_kind

            mol.natural_orbitals.destroy
            mol.natural_orbitals = mol.molecular_orbitals
           
            mol.occupation_numbers.destroy
            mol.occupation_numbers.create(.n_bf,ON_kind)
           
            mol.occupation_numbers.set_to_zero
            mol.:set_scf_occupations(ON_kind)

         end

      end
      end

   end

   assign_MOs_to_NOs ::: leaky
   ! Copy the MOs to be the same as the natural orbitals
   ! The occupation numbers are the MO ones.
      self :: INOUT

      ON_kind :: STR
      mol :: MOLECULE*
      g :: INT

      if (.natural_orbitals.allocated) then

         ON_kind = .natural_orbitals.occupation_spinorbital_kind

         .molecular_orbitals.destroy
         .molecular_orbitals = .natural_orbitals

         .occupation_numbers.destroy
         .occupation_numbers.create(ON_kind)

         .occupation_numbers.set_to_zero
         .:set_scf_occupations(ON_kind)

      end

      if (.atom_group.associated) then
      if (.atom_group(1).mol.associated) then
         
         do g = 1,.atom_group.dim

            mol => .atom_group(g).mol

            if (mol.natural_orbitals.deallocated) cycle

            ON_kind = .natural_orbitals.occupation_spinorbital_kind

            mol.molecular_orbitals.destroy
            mol.molecular_orbitals = mol.natural_orbitals
           
            mol.occupation_numbers.destroy
            mol.occupation_numbers.create(.n_bf,ON_kind)
           
            mol.occupation_numbers.set_to_zero
            mol.:set_scf_occupations(ON_kind)

         end

      end
      end

   end

