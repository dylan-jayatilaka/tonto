!  ==================
!  Read/write archive
!  ==================

   read_archive ::: leaky
   ! Read the archive called "name" with "genre", e.g.  "restricted".
   ! For use with TONTO generated archives.
   ENSURE(stdin.n_line_items==3, "must specify an archive and a kind")
   ENSURE(.n_bf>0, "need to already know the number of basis functions")

      name,genre :: STR

      stdin.read(name)
      stdin.read(genre)

      stdout.text("reading archive : " // trim(name))
      .:read_archive(name,genre)

   end

   read_archive(name,genre) ::: leaky
   ! Read the archive called "name" with "genre", e.g. "restricted".
   ! For use with TONTO generated archives.
      name,genre :: STR, IN

   ENSURE(.n_bf>0, "need to already know the number of basis functions")

      arch :: ARCHIVE

      ! Set the archive "name" & "genre"
      arch.set(.name,name,genre=genre)

      ! Read the archive
      select case (name)

         case ("molecular_orbitals")
            if (.molecular_orbitals.deallocated) .molecular_orbitals.create(.n_bf)
            arch.read(.molecular_orbitals)
            .molecular_orbitals.unnormalise("1",.first_basis_fn_for_shell,.last_basis_fn_for_shell,.angular_moment_for_shell)

         case ("density_matrix    ")
            if (.density_matrix.deallocated) .density_matrix.create(.n_bf)
            arch.read(.density_matrix)
            .density_matrix.unnormalise(.first_basis_fn_for_shell,.last_basis_fn_for_shell,.angular_moment_for_shell)

         case ("MP2_density_matrix")
            if (.density_matrix.deallocated) .density_matrix.create(.n_bf)
            arch.read(.density_matrix)
            .density_matrix.unnormalise(.first_basis_fn_for_shell,.last_basis_fn_for_shell,.angular_moment_for_shell)

         case ("MP3_density_matrix")
            if (.density_matrix.deallocated) .density_matrix.create(.n_bf)
            arch.read(.density_matrix)
            .density_matrix.unnormalise(.first_basis_fn_for_shell,.last_basis_fn_for_shell,.angular_moment_for_shell)

         case ("fock_matrix       ")
            if (.fock_matrix.deallocated) .fock_matrix.create(.n_bf)
            arch.read(.fock_matrix)
            .fock_matrix.unnormalise(.first_basis_fn_for_shell,.last_basis_fn_for_shell,.angular_moment_for_shell)

         case ("orbital_energies  ")
            if (.orbital_energies.disassociated) .orbital_energies.create(.n_bf)
            arch.read(.orbital_energies)

         case ("natural_orbitals  ")
            if (.natural_orbitals.deallocated) .natural_orbitals.create(.n_bf)
            arch.read(.natural_orbitals)
            .natural_orbitals.unnormalise("1",.first_basis_fn_for_shell,.last_basis_fn_for_shell,.angular_moment_for_shell)

         case ("occupation_numbers")
            if (.occupation_numbers.disassociated) .occupation_numbers.create(.n_bf)
            arch.read(.occupation_numbers)

         case ("covalent_orbitals ")
            if (.natural_orbitals.deallocated) .natural_orbitals.create(.n_bf)
            arch.read(.natural_orbitals)
            .natural_orbitals.unnormalise("1",.first_basis_fn_for_shell,.last_basis_fn_for_shell,.angular_moment_for_shell)

         case ("ionic_orbitals    ")
            if (.natural_orbitals.deallocated) .natural_orbitals.create(.n_bf)
            arch.read(.natural_orbitals)
            .natural_orbitals.unnormalise("1",.first_basis_fn_for_shell,.last_basis_fn_for_shell,.angular_moment_for_shell)

         case ("U_electric_dipole ")
            if (.U_electric_dipole.disassociated) .U_electric_dipole.create(.n_bf,.n_bf,3)
            arch.read(.U_electric_dipole)

         case ("U2_electric_dipole ")
            if (.U2_electric_dipole.disassociated) .U2_electric_dipole.create(.n_bf,.n_bf,6)
            arch.read(.U2_electric_dipole)

         case default
            UNKNOWN(name)

      end

      ! Clean up
      arch.close

   end

   read_ascii_archive ::: leaky
   ! Read the archive called "name" with "genre", e.g. "restricted".
   ENSURE(stdin.buffer.n_items==3, "must specify an archive and a kind")
   ENSURE(.n_bf>0, "need to already know the number of basis functions")

      name,genre :: STR
      arch :: ARCHIVE

      ! Read archive name & genre
      stdin.read(name)
      stdin.read(genre)

      ! Set archive "name", "genre" & ascii format
      arch.set(.name,name,genre=genre,format="ascii")

      ! Read the archive
      select case (name)

         case ("molecular_orbitals")
            if (.molecular_orbitals.deallocated) .molecular_orbitals.create(.n_bf)
            arch.read(.molecular_orbitals,by_column=TRUE)

         case ("density_matrix    ")
            if (.density_matrix.deallocated)     .density_matrix.create(.n_bf)
            arch.read(.density_matrix,by_column=TRUE)

         case ("MP2_density_matrix")
            if (.density_matrix.deallocated)     .density_matrix.create(.n_bf)
            arch.read(.density_matrix,by_column=TRUE)

         case ("MP3_density_matrix")
            if (.density_matrix.deallocated)     .density_matrix.create(.n_bf)
            arch.read(.density_matrix,by_column=TRUE)

         case ("orbital_energies  ")
            if (.orbital_energies.disassociated)   .orbital_energies.create(.n_bf)
            arch.read(.orbital_energies)

         case ("natural_orbitals  ")
            if (.natural_orbitals.deallocated)   .natural_orbitals.create(.n_bf)
            arch.read(.natural_orbitals,by_column=TRUE)

         case ("occupation_numbers")
            if (.occupation_numbers.disassociated) .occupation_numbers.create(.n_bf)
            arch.read(.occupation_numbers)

         case ("covalent_orbitals ")
            if (.natural_orbitals.deallocated)   .natural_orbitals.create(.n_bf)
            arch.read(.natural_orbitals,by_column=TRUE)

         case ("ionic_orbitals    ")
            if (.natural_orbitals.deallocated)   .natural_orbitals.create(.n_bf)
            arch.read(.natural_orbitals,by_column=TRUE)

         case ("fock_matrix       ")
            if (.fock_matrix.deallocated)        .fock_matrix.create(.n_bf)
            arch.read(.fock_matrix,by_column=TRUE)

         case default
            UNKNOWN(name)

      end

      ! Clean up
      arch.close

   end

   write_archive
   ! Write the archive called "name". The genre is defined by the
   ! object to be written.
      self :: INOUT

      name :: STR
      arch :: ARCHIVE

      ! Read the archive "name"
      stdin.read(name)

      ! Set archive "name"
      arch.set(.name,name)

      ! Write the archive, "genre" from object
      select case (name)

      case ("molecular_orbitals")
         DIE_IF(.molecular_orbitals.deallocated,"no molecular_orbitals")
         .molecular_orbitals.renormalise("1",.first_basis_fn_for_shell,.last_basis_fn_for_shell,.angular_moment_for_shell)
         arch.write(.molecular_orbitals)
         .molecular_orbitals.unnormalise("1",.first_basis_fn_for_shell,.last_basis_fn_for_shell,.angular_moment_for_shell)

      case ("density_matrix    ")
         DIE_IF(.density_matrix.deallocated,"no density_matrix")
         .density_matrix.renormalise(.first_basis_fn_for_shell,.last_basis_fn_for_shell,.angular_moment_for_shell)
         arch.write(.density_matrix)
         .density_matrix.unnormalise(.first_basis_fn_for_shell,.last_basis_fn_for_shell,.angular_moment_for_shell)

      case ("natural_orbitals  ")
         DIE_IF(.natural_orbitals.deallocated,"no natural_orbitals")
         .natural_orbitals.renormalise("1",.first_basis_fn_for_shell,.last_basis_fn_for_shell,.angular_moment_for_shell)
         arch.write(.natural_orbitals)
         .natural_orbitals.unnormalise("1",.first_basis_fn_for_shell,.last_basis_fn_for_shell,.angular_moment_for_shell)

      case ("occupation_numbers")
         DIE_IF(.occupation_numbers.disassociated,"no occupation_numbers")
         arch.write(.occupation_numbers)

      case ("fock_matrix       ")
         DIE_IF(.fock_matrix.deallocated,"no fock_matrix")
         .fock_matrix.renormalise(.first_basis_fn_for_shell,.last_basis_fn_for_shell,.angular_moment_for_shell)
         arch.write(.fock_matrix)
         .fock_matrix.unnormalise(.first_basis_fn_for_shell,.last_basis_fn_for_shell,.angular_moment_for_shell)

      case default
         UNKNOWN(name)

      end

      ! Clean up
      arch.close

   end

   write_ascii_archive
   ! Write the archive called "name". The genre is defined by the
   ! object to be written.

      name :: STR
