         nb = .n_ab - GAUSSIAN_DATA:n_comp(.ab_l_sum) ! No. of b's in int_new goes down
         allocate(int_new(nb,3))
            do b = 1,nb
               bb = b+fb
               b1 = GAUSSIAN_DATA::index_p1(bb,1) - fb
               b2 = GAUSSIAN_DATA::index_p1(bb,2) - fb
               b3 = GAUSSIAN_DATA::index_p1(bb,3) - fb
