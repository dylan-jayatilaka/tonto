               abcd(3,3) = escd(6)
         else                                   ! (ap|cd) case
            na = .a.n_comp
            fa = GAUSSIAN_DATA:n_comp_up_to(.a.l-1) ! Offset into the index array
            do a = 1,na                             ! Put (es|cd) straight to (ap|cd)
               aa = a+fa
               a1 = GAUSSIAN_DATA::index_p1(aa,1) - fa
               a2 = GAUSSIAN_DATA::index_p1(aa,2) - fa
               a3 = GAUSSIAN_DATA::index_p1(aa,3) - fa
