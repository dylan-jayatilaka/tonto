                 do bk1=1,.n_bf
                   .special_ket(k)[1] = bk1 
                   do bi2=1,.n_bf
                   do bq1=1,.n_bf
                     fac= sum(temp_qa_h2_ja_kb(:,bi2,bk1,bj2)*temp_bra_i(bq1,:))     &
