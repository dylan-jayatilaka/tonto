               do bj2=1,.n_bf
                 .special_ket(k)[2] = bj2
                 do bk1=1,.n_bf
                   fac= sum(temp_qa_ja(:,bj2)*temp_ket_k(bk1,:))+sum(temp_qb_jb(:,bk1)*temp_ket_k(:,bj2))
